page {
  background-color: #f5f5f5;
}
.bm .hang {
  padding: 20rpx 30rpx;
  align-items: flex-start;
  border-bottom: 1rpx solid #eee;
}
.bm .hang:last-child {
  padding-bottom: 0;
  border: 0;
}
.ul {
  background-color: #f5f5f5;
  padding: 20rpx;
}
.ul .li {
  padding: 20rpx 30rpx;
  border-radius: 10rpx;
  align-items: center;
  background-color: #ffffff;
  margin-bottom: 20rpx;
  box-shadow: 0px 0px  10px 5px #eee;
}
.ul .li .btn-style {
  width: 654rpx;
  height: 58rpx;
  line-height: 58rpx;
  background: #DEEBFF;
  border-radius: 29px;
  text-align: center;
  margin: 20rpx auto;
  color: #106DF7;
}
.ul .li .btn-style-end {
  background: #E9EAEA;
  color: #999999;
}
.ul .li .center {
  margin-left: 20rpx;
}
.ul .li .center .bd {
  font-size: 24rpx;
  margin-top: 20rpx;
}
.ul .li .center .bd .icoBox {
  margin-right: 15rpx;
}
.ul .li .center .desc {
  font-size: 24rpx;
  margin: 15rpx 0;
  color: #666666;
}
.ul .li .center .time {
  align-items: center;
  text-align: left;
  justify-content: flex-start;
  color: #FF4D4D;
  font-size: 24rpx;
}
.ul .li .center .time .color {
  color: #999999;
}
.ul .li .center .time .rt-sty {
  font-size: 22rpx;
  color: #106DF7;
  padding: 5rpx 20rpx;
  min-width: 80rpx;
  margin-left: 10rpx;
  background-color: #DEEBFF;
  border: 1rpx solid #106DF7;
  border-radius: 30rpx;
}
.ul .li .center .time .imgSty {
  width: 30rpx;
  height: 27rpx;
}
.ul .li:last-child {
  margin: 0;
}

