page {
  background-color: #f5f5f5;
}
.text-btn {
  background-color: transparent;
  font-size: 26rpx;
  padding: 0;
  margin: 0;
  color: #666;
}
.childBox .d-btn {
  margin-bottom: 20rpx;
}
button::after {
  border: none;
}
.ul {
  background-color: #f5f5f5;
  padding: 20rpx;
}
.ul .li {
  padding: 20rpx 30rpx;
  border-radius: 10rpx;
  align-items: center;
  background-color: #ffffff;
  margin-bottom: 20rpx;
  box-shadow: 0px 0px  10px 5px #eee;
}
.ul .li .t-box {
  border-top: 1rpx solid #eee;
  border-bottom: 1rpx solid #eee;
  padding: 20rpx 0;
}
.ul .li .t-title {
  margin-bottom: 20rpx;
}
.ul .li .t-title .t-1 .dec {
  font-size: 24rpx;
  color: #000;
}
.ul .li .t-title .t-1 .dec-r .state1 {
  color: #FF4D4D;
  display: inline-block;
}
.ul .li .t-title .t-1 .dec-r .state3 {
  color: #333;
}
.ul .li .t-title .t-1 .dec-r .state2 {
  display: inline-block;
  height: 58rpx;
  line-height: 58rpx;
  padding: 0 20rpx;
  background: #CBA668;
  border-radius: 10rpx;
  font-size: 24rpx;
  color: #fff;
}
.ul .li .d-btn {
  padding: 20rpx 0;
  padding-bottom: 0;
  justify-content: flex-end;
  font-size: 24rpx;
}
.ul .li .d-btn .text-btn {
  margin-left: 20rpx;
}
.ul .li .d-btn .text-1 {
  width: 160rpx;
  height: 58rpx;
  line-height: 58rpx;
  background: #FFFFFF;
  border: 1rpx solid #1370F7;
  border-radius: 10rpx;
  font-size: 24rpx;
}
.ul .li .d-btn .text-2 {
  width: 160rpx;
  height: 58rpx;
  line-height: 58rpx;
  background: #CBA668;
  border-radius: 10rpx;
  color: #fff;
  font-size: 24rpx;
}
.ul .li .d-btn .text-3 {
  width: 160rpx;
  height: 58rpx;
  line-height: 58rpx;
  background: linear-gradient(90deg, #1370F7 0%, #3484F8 100%);
  border-radius: 10rpx;
  color: #fff;
  font-size: 24rpx;
}
.ul .li .td-title {
  font-size: 30rpx;
  color: #333333;
}
.ul .li .center {
  margin-left: 20rpx;
}
.ul .li .center .hd {
  font-size: 28rpx;
}
.ul .li .center .bd {
  font-size: 30rpx;
  color: #333333;
  margin-top: 20rpx;
}
.ul .li .center .desc {
  font-size: 24rpx;
  margin: 15rpx 0;
  color: #666666;
}
.ul .li .center .time {
  align-items: center;
  text-align: center;
  justify-content: space-between;
  color: #FF4D4D;
  font-size: 24rpx;
}
.ul .li .center .time .color {
  color: #999999;
}
.ul .li .center .time .imgSty {
  width: 30rpx;
  height: 27rpx;
}
.ul .li:last-child {
  margin: 0;
}

