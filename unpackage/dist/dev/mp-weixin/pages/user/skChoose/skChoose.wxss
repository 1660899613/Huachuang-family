page {
  background-color: #f5f5f5;
}
.wrap .list {
  width: 100%;
  border-bottom: solid 1rpx #eee;
  padding: 20rpx;
  box-sizing: border-box;
}
.wrap .list .title {
  color: #333333;
  font-size: 32rpx;
  text-align: center;
  padding: 20rpx;
  padding-top: 0;
}
.wrap .list .row {
  width: 100%;
  height: 100rpx;
  display: flex;
  align-items: center;
}
.wrap .list .row .left {
  width: 65rpx;
  flex-shrink: 0;
  display: flex;
  align-items: center;
}
.wrap .list .row .left image {
  width: 80rpx;
  height: 80rpx;
}
.wrap .list .row .center {
  width: 100%;
  font-size: 30rpx;
  text-align: left;
  color: #333;
}
.wrap .list .row .center .tel {
  color: #999999;
  font-size: 24rpx;
}
.wrap .list .row .icon {
  flex-shrink: 0;
  display: flex;
  justify-content: flex-end;
  color: #999;
}
.wrap .list .row .right {
  width: 80rpx;
  flex-shrink: 0;
  display: flex;
  justify-content: flex-end;
}
.wrap .content .ul .li {
  margin-top: 20rpx;
  background-color: white;
  padding: 20rpx;
}
.wrap .content .ul .li .th {
  font-size: 30rpx;
}
.wrap .content .ul .li .addInfor {
  color: #CBA668;
  font-size: 26rpx;
  padding: 20rpx 0;
  text-align: center;
}
.wrap .content .ul .li .btn {
  width: 700rpx;
  height: 90rpx;
  line-height: 90rpx;
  background: #CBA668;
  border-radius: 15rpx;
  color: #fff;
  text-align: center;
}
.wrap .content .ul .li .row1 {
  background-color: #fff;
}
.wrap .content .ul .li .row1 .row {
  justify-content: space-between;
}
.wrap .content .ul .li .row1 view {
  display: flex;
}
.wrap .content .ul .li .row1 .input {
  text-align: right;
}
.wrap .content .ul .li .row1 .input input {
  width: 100%;
}
.wrap .header {
  padding: 20rpx;
  background-color: #fff;
}
.wrap .header .t-box {
  border-top: 1rpx solid #eee;
  border-bottom: 1rpx solid #eee;
  padding: 20rpx 0;
}
.wrap .header .t-box .center {
  margin-left: 20rpx;
}
.wrap .header .t-box .center .hd {
  font-size: 28rpx;
}
.wrap .header .t-box .center .bd {
  font-size: 30rpx;
  color: #333333;
  margin-top: 20rpx;
}
.wrap .header .t-box .center .desc {
  font-size: 24rpx;
  margin: 15rpx 0;
  color: #666666;
}
.wrap .header .t-box .center .time {
  align-items: center;
  text-align: center;
  justify-content: space-between;
  color: #FF4D4D;
  font-size: 24rpx;
}
.wrap .header .t-box .center .time .color {
  color: #999999;
}
.wrap .header .t-box .center .time .imgSty {
  width: 30rpx;
  height: 27rpx;
}
.wrap .row {
  widows: 100%;
  min-height: 90rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  border-bottom: solid 1rpx #eee;
}
.wrap .row:last-child {
  border-bottom: none;
}
.wrap .row .title {
  color: #333;
}
.wrap .row .right {
  display: flex;
  align-items: center;
  color: #999;
  text-align: right;
  justify-content: flex-end;
}
.wrap .row .right .tis {
  margin-right: 5rpx;
  max-height: 120rpx;
  text-align: right;
}
.wrap .row .right .tis image {
  width: 100rpx;
  height: 100rpx;
  border-radius: 100%;
  margin: 10rpx 0;
}
.wrap .row .right .icon {
  width: 40rpx;
  color: #cecece;
}

