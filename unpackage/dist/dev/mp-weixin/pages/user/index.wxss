page {
  background-color: #f5f5f5;
}
.u-nav-slot {
  font-size: 34rpx;
  font-weight: bold;
  color: #fff;
}
.ad-banner {
  width: 700rpx;
  height: 142rpx;
  margin: 20rpx auto;
  text-align: center;
  box-sizing: border-box;
  margin-top: 185rpx;
}
.text {
  background-color: transparent;
  font-size: 26rpx;
  padding: 0;
  margin: 0;
  color: #666;
}
button::after {
  border: none;
}
.listSet {
  background-color: white;
  width: 700rpx;
  border-radius: 20rpx;
  padding: 30rpx;
  padding-bottom: 10rpx;
  margin: 0 auto;
  box-sizing: border-box;
}
.listSet .title {
  color: #333333;
  padding-left: 20rpx;
}
.listSet .ul {
  flex-wrap: wrap;
  justify-content: flex-start;
  align-items: center;
  margin: 30rpx 0;
  font-size: 26rpx;
  color: #666666;
}
.listSet .ul .li {
  width: 25%;
  margin-bottom: 20rpx;
  text-align: center;
}
.listSet .ul .li .img-box {
  width: 55rpx;
  height: 56rpx;
  margin: auto;
}
.listSet .ul .li .text {
  margin-top: 10rpx;
  border: 0;
}
.header {
  width: 750rpx;
  height: 430rpx;
  background: linear-gradient(166.26deg, #5DA4F4 0%, #EFF2F8 100%);
  position: relative;
}
.header .headContent {
  width: 700rpx;
  height: 330rpx;
  border-radius: 20rpx;
  background-color: white;
  position: absolute;
  bottom: -165rpx;
  left: 25rpx;
  box-sizing: border-box;
  padding: 30rpx 50rpx;
}
.header .headContent .top-tx {
  position: absolute;
  top: -60rpx;
  left: 50rpx;
}
.header .headContent .hd .title {
  font-size: 34rpx;
  color: #333333;
  padding-top: 50rpx;
}
.header .headContent .hd .desc {
  color: #666666;
  font-size: 24rpx;
  margin-top: 10rpx;
}
.header .headContent .ul {
  justify-content: center;
  align-items: center;
  margin: 30rpx 0;
  font-size: 26rpx;
  color: #666666;
}
.header .headContent .ul .li {
  text-align: center;
}
.header .headContent .ul .li .img-box {
  width: 55rpx;
  height: 56rpx;
  margin: auto;
}
.header .headContent .ul .li .text {
  margin-top: 10rpx;
}

