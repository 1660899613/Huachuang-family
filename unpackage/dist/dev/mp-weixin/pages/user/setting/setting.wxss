page {
  background-color: #f3f3f3;
}
.footer {
  position: fixed;
  bottom: 20rpx;
  left: 20rpx;
  width: 710rpx;
  height: 100rpx;
  line-height: 100rpx;
  border-radius: 10rpx;
  text-align: center;
  color: #fff;
  background: linear-gradient(90deg, #1370F7 0%, #3484F8 100%);
}
.icon {
  font-size: 30rpx;
}
.content {
  padding-bottom: 20rpx;
}
.content .list {
  border-radius: 20rpx;
  padding: 20rpx;
  background-color: #fff;
  margin-bottom: 20rpx;
}
.content .list .row {
  widows: 100%;
  min-height: 90rpx;
  padding: 10rpx 0;
  display: flex;
  align-items: center;
  justify-content: space-between;
  border-bottom: solid 1rpx #eee;
}
.content .list .row:last-child {
  border-bottom: none;
}
.content .list .row .title {
  font-size: 32rpx;
  color: #333;
}
.content .list .row .right {
  display: flex;
  align-items: center;
  color: #999;
}
.content .list .row .right .tis_sex {
  font-size: 30rpx !important;
  color: #333;
}
.content .list .row .right .tis {
  font-size: 26rpx;
  margin-right: 5rpx;
  max-height: 120rpx;
  text-align: right;
}
.content .list .row .right .tis image {
  width: 100rpx;
  height: 100rpx;
  border-radius: 100%;
  margin: 10rpx 0;
}
.content .list .row .right .icon {
  width: 40rpx;
  color: #cecece;
}
.list_sex {
  width: 100%;
  border-bottom: solid 1rpx #eee;
  padding: 20rpx;
  box-sizing: border-box;
}
.list_sex .title {
  color: #333333;
  font-size: 32rpx;
  text-align: center;
  padding: 20rpx;
  padding-top: 0;
}
.list_sex .row {
  width: 100%;
  height: 100rpx;
  display: flex;
  align-items: center;
}
.list_sex .row .left {
  width: 65rpx;
  flex-shrink: 0;
  display: flex;
  align-items: center;
}
.list_sex .row .left image {
  width: 80rpx;
  height: 80rpx;
}
.list_sex .row .center {
  width: 100%;
  font-size: 30rpx;
  text-align: left;
  color: #333;
}
.list_sex .row .center .tel {
  color: #999999;
  font-size: 24rpx;
}
.list_sex .row .icon {
  flex-shrink: 0;
  display: flex;
  justify-content: flex-end;
  color: #999;
}
.list_sex .row .right {
  width: 80rpx;
  flex-shrink: 0;
  display: flex;
  justify-content: flex-end;
}

