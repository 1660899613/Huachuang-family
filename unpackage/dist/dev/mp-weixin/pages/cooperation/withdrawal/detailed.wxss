page {
  background-color: #f5f5f5;
}
.wrap .ul-list {
  background-color: #fff;
}
.wrap .ul-list .li {
  padding: 20rpx;
  border-bottom: 1rpx solid #eee;
}
.wrap .ul-list .li .left {
  color: #7C7C7C;
}
.wrap .ul-list .li .left .title {
  font-size: 30rpx;
  color: #333333;
}
.wrap .ul-list .li .right {
  font-size: 34rpx;
}
.wrap .header {
  padding: 20rpx;
  margin-bottom: 20rpx;
  background-color: #fff;
}
.wrap .header .left .title {
  font-size: 30rpx;
  margin-bottom: 20rpx;
}
.wrap .header .left .des {
  color: #666666;
  font-size: 26rpx;
}
.wrap .header .right {
  width: 161rpx;
  height: 66rpx;
  line-height: 66rpx;
  background: #1370F7;
  border-radius: 33rpx;
  text-align: center;
  color: #fff;
}

